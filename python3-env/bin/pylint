#!/workspace/theia/python3-env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pylint==2.4.4','console_scripts','pylint'
__requires__ = 'pylint==2.4.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pylint==2.4.4', 'console_scripts', 'pylint')()
    )
